    "@types/express": "^4.17.13",
    "@types/node": "^16.10.2",
    "@types/source-map-support": "^0.5.4",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nanoid": "^3.1.28",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
